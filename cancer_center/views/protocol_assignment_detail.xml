<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="cancer_center_protocol_assignment_detail_view_form" model="ir.ui.view">
        <field name="name">cancer_center.protocol.assignment.detail.view.form</field>
        <field name="model">cancer_center.protocol.assignment.detail</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="day_number"></field>
                    </group>
                    <group>
                        <field name="madication_id"></field>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="cancer_center_protocol_assignment_detail_action" model="ir.actions.act_window">
        <field name="name">protocol.assignment.detail</field>
        <field name="res_model">cancer_center.protocol.assignment.detail</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
