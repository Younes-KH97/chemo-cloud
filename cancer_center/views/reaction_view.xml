<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="cancer_center_reaction_view_form" model="ir.ui.view">
        <field name="name">cancer_center.reaction.view.form</field>
        <field name="model">cancer_center.reaction</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="degre" />
                    </group>
                    <group>
                        <field name="comment" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="cancer_center_reaction_view_tree" model="ir.ui.view">
        <field name="name">cancer_center.reaction.view.tree</field>
        <field name="model">cancer_center.reaction</field>
        <field name="arch" type="xml">
            <tree string="" editable="bottom">
                <field name="degre" />
                <field name="comment" />
            </tree>
        </field>
    </record>

    <record id="cancer_center_reaction_action" model="ir.actions.act_window">
        <field name="name">Action Name</field>
        <field name="res_model">cancer_center.reaction</field>
        <field name="view_mode">tree,form</field>
        <!-- <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            
        </field> -->
    </record>
</odoo>
